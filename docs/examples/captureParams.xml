<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<parameterSetList
   xmlns="http://ns.blankaspect.uk/parameterSetList-1"
   version="0"
   application="crosswordEditor">

  <parameterSet
     name="FT">
    <property
       key="answerLengthPattern"
       value="\((\d[^\(]*?)\)$"/>
    <property
       key="automaticGridDetection"
       value="true"/>
    <property
       key="barBrightnessThreshold"
       value="60"/>
    <property
       key="barWidthThreshold"
       value="3"/>
    <property
       key="blockBrightnessThreshold"
       value="60"/>
    <property
       key="clueReferenceKeyword"
       value="See"/>
    <property
       key="clueSubstitution.000"
       value="/–/—/l"/>
    <property
       key="clueSubstitution.001"
       value="/.../…/l"/>
    <property
       key="clueSubstitution.002"
       value="/\uFB00/ff/"/>
    <property
       key="filename"
       value="ft180%n"/>
    <property
       key="gridLine.brightnessThreshold"
       value="60"/>
    <property
       key="gridLine.endpointTolerance"
       value="1"/>
    <property
       key="gridLine.minLength"
       value="200"/>
    <property
       key="gridLine.minSeparation"
       value="16"/>
    <property
       key="gridSeparator"
       value="block"/>
    <property
       key="multipleFieldClueIdSeparator"
       value="/"/>
    <property
       key="numColumns"
       value="15"/>
    <property
       key="numRows"
       value="15"/>
    <property
       key="sampleSize"
       value="1"/>
    <property
       key="title"
       value="FT 180%n | "/>
    <property
       key="xOffset"
       value="10"/>
    <property
       key="yOffset"
       value="10"/>
  </parameterSet>

